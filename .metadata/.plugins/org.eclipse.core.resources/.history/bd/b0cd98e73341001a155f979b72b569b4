#include <stdio.h>

int main(){
    int numero;

    printf("informe um número:  ");
    fflush(stdout);
    scanf("%d", &numero);

    while(numero < 1 || numero > 10){
        printf("Número deve ser entre 1 e 10\n");
        printf("Informe um número:  ");
        fflush(stdout);
        scanf("%d", &numero);
    }
    for(int i = 1; i <= 10; i++){
        printf("%d x %d = %d\n",numero, i, (numero * i));

    }
}
