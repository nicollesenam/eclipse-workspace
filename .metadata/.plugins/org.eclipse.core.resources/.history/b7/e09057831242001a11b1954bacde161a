#include <stdio.h>
#include <string.h>

int main(){
	char nome[100], senha[100];

	printf("Informe seu nome de usuário:  ");
	gets(nome);
	printf("Informe sua senha:  ");
	gets(senha);

	while(!strcmp(nome, senha)){
		printf("Nome de usuário tem que ser diferente da senha\n");
		printf("Informe seu nome de usuário:  ");
		gets(nome);
		printf("Informe sua senha:  ");
		gets(senha);
	}




}
