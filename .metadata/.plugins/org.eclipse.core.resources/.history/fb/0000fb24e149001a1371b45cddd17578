package br.com.geekuniversitysecao08;
//jeito dele de fazer
import java.util.Scanner;

public class Exercicio05dnv {

	public static void main(String[] args) {
		int[] vetor = new int[10];
		boolean maior_50 = false;
		//variavel vai dizer se tem ou n numeros maiores q 50
		Scanner teclado = new Scanner(System.in);
		
		//entradas//processamento
		for(int i = 0; i < 10; i++) {
			System.out.printf("Informe o valor para o vetor %d/10\n", (i + 1));
			
		}
		for(int i = 0; i < 10; i++) {
			if(vetor[i] > 50) {
				System.out.printf("O número %d está na posição %d\n", vetor[i], i);
			}
		}
		


	}

}
